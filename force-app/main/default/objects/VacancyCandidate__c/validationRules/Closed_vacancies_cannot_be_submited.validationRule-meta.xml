<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Closed_vacancies_cannot_be_submited</fullName>
    <active>true</active>
    <errorConditionFormula>OR( TEXT(Vacancy__r.Status__c) = &#39;Cancelled&#39;, TEXT(Vacancy__r.Status__c)= &#39;Closed&#39;)</errorConditionFormula>
    <errorMessage>Closed and canceled vacancies not allowed for proposal</errorMessage>
</ValidationRule>
